<h1> Evaluation Sessions </h1>
<table>
  <thead>
    <tr>
      <th> Name </th>
      <th> Description </th>
      <th> Item type </th>
    </tr>
  </thead>

  <tbody>
    <% @sessions.each do |s| %>
    <tr>
      <td><%= link_to s.name, session_path(s) %></td>
      <td><%= s.description %> </td>
      <td><%= s.item_type %> </td>
      <td><%= link_to "Add items to evaluate", new_session_item_path(s.id), class: 'button' %></td>
    </tr>
    <br>
    <% end %>
  </tbody>
</table>
