<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Item type</th>
    </tr>
  </thead>
  <tbody>
    <tr>
      <td><%= @session.name %></td>
      <td><%= @session.item_type %></td>
    </tr>
  </tbody>
</table>

<%= simple_form_for [@session, @session.items.build] do |f| %>
  <%= f.input :name %>
  <%= f.input :type, placeholder: 'car', :readonly => true %>
  <%= f.submit %>
<% end %>
